let n = load_cstore[{<id: int, name: string, imdb_index: string, imdb_id: string, gender: string, name_pcode_cf: string, name_pcode_nf: string, surname_pcode: string, md5sum: string> -> int}]("datasets/job/7a/n.csv")
let pi = load_cstore[{<id: int, person_id: int, info_type_id: int, info: string, note: string> -> int}]("datasets/job/7a/pi.csv")
let it = load_cstore[{<id: int, info: string> -> int}]("datasets/job/7a/it.csv")
let t = load_cstore[{<id: int, title: string, imdb_index: string, kind_id: int, production_year: int, imdb_id: string, phonetic_code: string, episode_of_id: int, season_nr: int, episode_nr: int, series_years: string, md5sum: string> -> int}]("datasets/job/7a/t.csv")
